<p id="notice"><%= notice %></p>

<h1>Unit List Lines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unit_list_lines.each do |unit_list_line| %>
      <tr>
        <td><%= unit_list_line.name %></td>
        <td><%= unit_list_line.value %></td>
        <td><%= link_to 'Show', unit_list_line %></td>
        <td><%= link_to 'Edit', edit_unit_list_line_path(unit_list_line) %></td>
        <td><%= link_to 'Destroy', unit_list_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unit List Line', new_unit_list_line_path %>
