<%= form_with(model: params[:program_id] != nil ? [@program, @analyte, data_list] : data_list, local: true) do |form| %>
  <% if data_list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(data_list.errors.count, "error") %> prohibited this data_list from being saved:</h2>

      <ul>
      <% data_list.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :data_list_name %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
