<h2 align="center">Analitler</h2></br>
<% @analytes.each do |analyte| %>
  <div class="col-md-4">
    <div align="center" class="message">
          <h2 align="center" class="message-title"><%= link_to analyte.name, program_analyte_path(@program, analyte) %></h2>
          <div class="btn-group" role="group" aria-label="Basic example">
            <button type="button" class="btn btn-default"><%= link_to 'Unit List', analyte.unit_list == nil ? new_program_analyte_unit_list_path(@program, analyte) : program_analyte_unit_list_path(@program, analyte, analyte.unit_list)%></button>
            <button type="button" class="btn btn-default"><%= link_to 'Mode List', analyte.mode_list == nil ? new_program_analyte_mode_list_path(@program, analyte) : program_analyte_mode_list_path(@program, analyte, analyte.mode_list)%></button>
            <button type="button" class="btn btn-default"><%= link_to 'Reagent List', analyte.reagent_list == nil ? new_program_analyte_reagent_list_path(@program, analyte) : program_analyte_reagent_list_path(@program, analyte, analyte.reagent_list)%></button>
          <div class="text-center">
            <button type="button" class="btn btn-default"><%= link_to 'Data List', analyte.data_list == nil ? new_program_analyte_data_list_path(@program, analyte) : program_analyte_data_list_path(@program, analyte, analyte.data_list)%></button>
          </div>
        </div>
    </div>
  </div>
<% end %>
<div class="col-md-4">
  <div align="center" class="message">
    <h1 align="center" class="message-title">
      <button type="button" class="btn btn-secondary btn-lg btn-block">
        <%= link_to 'Yeni Analit Oluştur', new_program_analyte_path %>
      </button>
    </h1>
    <h2 align="center" class="message-title">
      <button type="button" class="btn btn-secondary btn-lg btn-block">
      <%= link_to 'Programlara Dön', {controller: "programs", action: "index" } %>
      </button>
    </h2>
  </div>
</div>
